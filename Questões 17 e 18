#include <stdio.h>


//farenheit para celsius
float celciusPortioli(float x){
 return ((x-32)*5)/9;
}


//celsius para farenheit
float CallMeMisterFahrenheit(float x){
  return ((x*9)/5)+32;
}

int main(void) {
  float x;
  scanf("%f", &x);
  printf("%f\n%f\n", celciusPortioli(x), CallMeMisterFahrenheit(x));
  return 0;
}
